{
  "name": "kamand-back",
  "version": "2.0.38",
  "description": "Kamand back end",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karianpour/kamand-back"
  },
  "private": false,
  "scripts": {
    "build": "rm -rf ./dist && tsc --build ./tsconfig.build.json",
    "start": "TS_NODE_FILES=true node --inspect=5858 -r ts-node/register ./src/example/index.ts",
    "start:watch": "nodemon",
    "build-tsc": "tsc",
    "start-ts": "ts-node ./src/example/index.ts",
    "build-dev": "rm -rf ./dev && tsc",
    "start-old": "rm -rf ./dev && tsc --watch & nodemon dev/example/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "engines": {
    "node": ">=8"
  },
  "author": "Kayvan Arianpour",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.6.3",
    "@types/node": "^12.12.36",
    "@types/pg": "^7.14.3",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/pg-format": "^1.0.0",
    "change-case": "^4.1.1",
    "debug": "^4.1.0",
    "dotenv": "^8.2.0",
    "fastify": "^2.13.1",
    "fastify-cors": "^3.0.3",
    "fastify-file-upload": "^2.0.1",
    "fastify-jwt": "^1.3.1",
    "fastify-plugin": "^1.6.1",
    "pg": "^8.0.2",
    "pg-cursor": "^2.1.9",
    "pg-format": "^1.0.4",
    "socket.io": "^2.3.0",
    "sql-bricks-postgres": "^0.5.0"
  }
}
