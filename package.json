{
  "name": "kamand-back",
  "version": "3.0.49",
  "description": "Kamand back end",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karianpour/kamand-back"
  },
  "private": false,
  "scripts": {
    "build": "rm -rf ./dist && tsc --build ./tsconfig.build.json",
    "start": "TS_NODE_FILES=true node --inspect=5858 -r ts-node/register src/example/index.ts",
    "start:watch": "nodemon",
    "build-tsc": "tsc",
    "start-ts": "ts-node src/example/index.ts",
    "build-dev": "rm -rf ./dev && tsc",
    "start-old": "rm -rf ./dev && tsc --watch & nodemon dev/example/index.js",
    "test": "TS_NODE_FILES=true tap  --bail --no-ts --node-arg=--require=ts-node/register --before=src/example/setup.js --after=src/example/teardown.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "engines": {
    "node": ">=8"
  },
  "author": "Kayvan Arianpour",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^12.12.53",
    "@types/pg": "^7.14.4",
    "@types/pg-format": "^1.0.0",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^8.0.0",
    "axios": "^0.19.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "tap": "^14.10.8",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "debug": "^4.1.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.1.1",
    "fastify-cors": "^4.1.0",
    "fastify-file-upload": "^3.0.0",
    "fastify-jwt": "^2.1.2",
    "fastify-plugin": "^2.0.2",
    "fastify-swagger": "^3.1.2",
    "pg": "^8.3.0",
    "pg-format": "^1.0.4",
    "socket.io": "^2.3.0",
    "sql-bricks-postgres": "^0.5.0",
    "uuid": "^8.2.0"
  }
}
