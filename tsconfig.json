{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dev",
    "lib": ["es2015", "dom", "es2017"],
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "sourceMap": true,
    "declaration": false,
  },
  "include": [
    "src/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
